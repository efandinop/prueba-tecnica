<odoo>
    <record id="view_customer_preferences_form" model="ir.ui.view">
        <field name="name">bookstore.customer.preferences.form</field>
        <field name="model">bookstore.customer.preferences</field>
        <field name="arch" type="xml">
            <form string="Preferencias del Cliente">
                <sheet>
                    <group>
                        <field name="customer_id"/>
                        <field name="preferred_genres"/>
                        <field name="frequent_buyer"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
